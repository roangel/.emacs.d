#ifndef CRAZYFLY_H
#define CRAZYFLY_H

#include "globalDefinitions.h"

class linkedCF
{
public:
    explicit linkedCF();
    ~linkedCF();

private:

    // info to fill by message
    std::string m_name;
    qreal m_x;
    qreal m_y;
    qreal m_z;

    qreal m_roll;
    qreal m_pitch;
    qreal m_yaw;

    // info for plotting CF
    qreal m_width;
    qreal m_height;
};


#endif
